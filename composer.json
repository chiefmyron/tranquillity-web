{
    "name": "tranquillity/tranquillity-web",
    "description": "Tranquillity Web Client",
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Tranquillity\\": "src/classes/"
        },
        "files": [
            "src/application/helpers.php"
        ]
    },
    "require": {
        "monolog/monolog": "^2.0",
        "php-di/php-di": "^6.0",
        "slim/psr7": "^0.5.0",
        "slim/slim": "^4.2",
        "slim/twig-view": "^3.1",
        "symfony/finder": "^5.1",
        "vlucas/phpdotenv": "^5.1"
    }
    ,
    "require-dev": {
        "roave/security-advisories": "dev-master"
    }
}
