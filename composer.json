{
    "name": "tranquility/tranquility-web",
    "description": "Tranquility Web Client",
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src",
            "Tranquility\\": "src/classes/"
        },
        "files": [
            "src/application/helpers.php"
        ]
    },
    "require": {
        "monolog/monolog": "^2.0",
        "php-di/php-di": "^6.0",
        "slim/psr7": "^0.5.0",
        "slim/slim": "^4.2",
        "symfony/finder": "^5.1",
        "twig/twig": "^3.0",
        "vlucas/phpdotenv": "^5.1"
    }
    ,
    "require-dev": {
        "roave/security-advisories": "dev-master"
    }
}
